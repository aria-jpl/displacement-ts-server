{
  "datasets": [
    { 
      "ipath": "ariamh::data/area_of_interest",
      "match_pattern": "/(?P<id>AOI_.+)$",
      "alt_match_pattern": null,
      "extractor": null,
      "level": "l0",
      "type": "area_of_interest",
      "publish": {
        "s3-profile-name": "default",
        "location": "s3://s3-us-west-2.amazonaws.com:80/grfn-v2-ops-product-bucket/datasets/area_of_interest/${version}/${id}",
